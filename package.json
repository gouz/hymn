{
  "name": "hymn",
  "version": "1.0.0",
  "description": "music generator for names",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouz/hymn.git"
  },
  "author": "Sylvain Gougouzian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gouz/hymn/issues"
  },
  "homepage": "https://github.com/gouz/hymn#readme",
  "source": [
    "src/index.pug"
  ],
  "scripts": {
    "preparcel": "rm -rf dist docs .parcel-cache",
    "parcel": "parcel $RUN",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs --public-url \"./\"",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "2.2.1",
    "@parcel/compressor-gzip": "2.2.1",
    "@parcel/optimizer-css": "2.2.1",
    "@parcel/transformer-css-experimental": "2.2.1",
    "@parcel/transformer-less": "2.2.1",
    "@parcel/transformer-pug": "2.2.1",
    "@prettier/plugin-pug": "1.19.2",
    "abcjs": "5.12.0",
    "autoprefixer": "10.4.2",
    "eslint": "8.8.0",
    "parcel": "2.3.0",
    "parcel-reporter-static-files-copy": "1.3.4",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.19"
  },
  "dependencies": {}
}
