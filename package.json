{
  "name": "hymn",
  "version": "1.0.0",
  "description": "music generator for names",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouz/hymn.git"
  },
  "author": "Sylvain Gougouzian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gouz/hymn/issues"
  },
  "homepage": "https://github.com/gouz/hymn#readme",
  "source": [
    "src/index.pug"
  ],
  "scripts": {
    "preparcel": "rm -rf dist docs .parcel-cache",
    "parcel": "parcel $RUN",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs --public-url \"./\"",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "2.5.0",
    "@parcel/compressor-gzip": "2.5.0",
    "@parcel/optimizer-css": "2.5.0",
    "@parcel/transformer-css-experimental": "2.3.2",
    "@parcel/transformer-less": "2.5.0",
    "@parcel/transformer-pug": "2.5.0",
    "@prettier/plugin-pug": "2.0.0",
    "abcjs": "6.0.2",
    "autoprefixer": "10.4.7",
    "buffer": "6.0.3",
    "eslint": "8.14.0",
    "parcel": "2.5.0",
    "parcel-reporter-static-files-copy": "1.3.4",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24"
  }
}
